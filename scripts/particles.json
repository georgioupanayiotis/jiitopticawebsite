{
  "particles": {
    "number": {
      "value": 250
    },
    "color": {
      "value": "#ffffff"
    },
    "shape": {
      "type": "circle",
      "stroke": {
        "width": 1,
        "color": "#171c28"
      }
    },
    "opacity": {
      "value": 0.5,
      "random": true,
      "anim": {
        "enable": false,
        "speed": 1
      }
    },
    "size": {
      "value": 5,
      "random": false,
      "anim": {
        "enable": true,
        "speed": 30
      }
    },
    "interactivity": {
      "events": {
        "onhover": {
          "enable": true,
          "mode": "repulse"
        },
        "onclick": {
          "enable": true,
          "mode": "bubble"
        }
      }
    },
    "modes": {
      "repulse": {
        "distance": 50,
        "duration": 0.4
      },
      "bubble": {
        "distance": 100,
        "size": 10
      }
    }
  }
}
